{
  "cli": {
    "version": ">= 16.4.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
         "EXPO_PUBLIC_BACKEND_URL": "https://footballtracker-backend.onrender.com/"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
         "EXPO_PUBLIC_BACKEND_URL": "https://footballtracker-backend.onrender.com/"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
         "EXPO_PUBLIC_BACKEND_URL": "https://footballtracker-backend.onrender.com/"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
