{
  "expo": {
    "name": "FootBall Tracker",
    "slug": "mobile-front",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/RedBlackLogo.png",
    "backgroundColor": "#22252B",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/RedWhiteLogo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/BlackSplashLogo.png",
          "backgroundColor": "#D9DADC",
          "dark": {
              "image": "./assets/images/WhiteSplashLogo.png",
              "backgroundColor": "#191A1C"
          },
          "resizeMode": "contain",
          "imageWidth": 200,
          "android": {
            "image": "./assets/images/BlackSplashLogo.png",
            "backgroundColor": "#D9DADC",
            "dark": {
                "image": "./assets/images/WhiteSplashLogo.png",
                "backgroundColor": "#191A1C"
            }
          }
        }
      ],
      "expo-font",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
